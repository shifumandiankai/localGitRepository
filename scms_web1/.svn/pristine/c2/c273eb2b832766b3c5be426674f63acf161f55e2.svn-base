$.ajax({
	 url: "/report/configentranceName.do",
     dataType: 'json',
     type:'post',
     success: function (data) {
     	var result=eval(data);
     	if(result.length!=0){
     		for(var i in result){
         	$('select[name="entranceName"]').append('<option value="'+result[i].id+'">'+result[i].text+'</option>');	
         	}
 		}
     }
});

