public class ChargeRuleDto{
	private int chargeRuleId;	//规则ID 
	private String ruleName;	//规则名称
	private int  carTypeId;  //车辆类型ID
	private int ruleType; //收费方式
	private long updateTime; //更新时间
	private int  tempMatch;
	
	public int getChargeRuleId() {
		return chargeRuleId;
	}
	public void setChargeRuleId(int chargeRuleId) {
		this.chargeRuleId = chargeRuleId;
	}
	public String getRuleName() {
		return ruleName;
	}
	public void setRuleName(String ruleName) {
		this.ruleName = ruleName;
	}
	public int getCarTypeId() {
		return carTypeId;
	}
	public void setCarTypeId(int carTypeId) {
		this.carTypeId = carTypeId;
	}
	public int getRuleType() {
		return ruleType;
	}
	public void setRuleType(int ruleType) {
		this.ruleType = ruleType;
	}
	public long getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime.getTime();
	}
	public int getTempMatch() {
		return tempMatch;
	}
	public void setTempMatch(int tempMatch) {
		this.tempMatch = tempMatch;
	}
}
