.clearfix {
  *zoom: 1;
}
.clearfix:before,
.clearfix:after {
  display: table;
  line-height: 0;
  content: "";
}
.clearfix:after {
  clear: both;
}
.homePageNav {
  margin-bottom: 20px;
  margin-left: 0;
  list-style: none;
}
.homePageNav > li > a {
  display: block;
}
.homePageNav > li > a:hover {
  text-decoration: none;
  background-color: #eeeeee;
}

.homePageNav > li > a > img {
  max-width: none;
}

.homePageNav > .pull-right {
  float: right;
}

.homePageNav-tabs,
.homePageNav-pills {
  *zoom: 1;
}
.homePageNav-tabs:before,
.homePageNav-pills:before,
.homePageNav-tabs:after,
.homePageNav-pills:after {
  display: table;
  line-height: 0;
  content: "";
}
.homePageNav-tabs:after,
.homePageNav-pills:after {
  clear: both;
}
.homePageNav-tabs > li,
.homePageNav-pills > li {
  float: left;
}

.homePageNav-tabs > li > a,
.homePageNav-pills > li > a {
  padding-right: 12px;
  padding-left: 12px;
  margin-right: 2px;
  line-height: 14px;
